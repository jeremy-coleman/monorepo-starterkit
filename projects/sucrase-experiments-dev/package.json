{
  "name": "coglite-app",
  "private": true,
  "version": "0.0.1",
  "main": "src/desktop/main.ts",
  "bin": {
    "sucrase-serve": "./cli.js"
  },
  "scripts": {
    "start": "electron .",
    "rebuild": "electron-rebuild",
    "pkg": "gulp build",
    "peek-tasks": "gulp --tasks-simple",
    "ssr:start": "sucrase-node ./src/ssr/cli.ts src/docs/App.tsx --bundle",
    "ssr:async": "sucrase-node ./src/ssr/cli.ts src/docs/Async",
    "ssr:styled": "sucrase-node ./src/ssr/cli.ts src/docs/StyledComponents",
    "ssr:bundle": "sucrase-node ./src/ssr/cli.ts src/docs/Bundle.tsx --bundle",
    "ssr:icon": "sucrase-node ./src/ssr/cli.ts src/docs/Icon.js -n"
  },
  "author": "jeremy",
  "license": "MIT",
  "dependencies": {
    "browserify": "16.5.0",
    "clsx": "1.0.4",
    "dotenv": "8.2.0",
    "electron-rebuild": "1.8.8",
    "find-up": "4.1.0",
    "gulp": "4.0.2",
    "json-stringify-safe": "5.0.1",
    "meow": "6.0.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "redux": "4.0.5",
    "styled-components": "4.4.1",
    "styled-system": "5.1.4",
    "sucrase": "3.11.0",
    "terser": "4.4.3",
    "tinyify": "2.5.2",
    "update-notifier": "4.0.0",
    "watchify": "3.11.1"
  },
  "devDependencies": {
    "@types/node": "13.1.1",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "cxs": "6.2.0",
    "electron": "7.1.7",
    "node-fetch": "2.6.0",
    "tslint": "5.20.1",
    "typescript": "3.7.4",
    "typescript-tslint-plugin": "0.5.5"
  }
}
